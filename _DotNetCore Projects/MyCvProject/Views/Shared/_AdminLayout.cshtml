@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 3 | Project Add</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <a href="/Default/Index/" class="brand-link">
                <img src="/AdminLTE-3.0.4/dist/img/AdminLTELogo.png"
                     alt="AdminLTE Logo"
                     class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Siteye Git</span>
            </a>
            <div class="sidebar">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/AdminLTE-3.0.4/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Aydın YILDIZ</a>
                    </div>
                </div>
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview">
                            <a href="/About/Index/" class="nav-link">
                                <i class="nav-icon fas fa-book"></i>
                                <p>
                                    About
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Experience/Index/" class="nav-link">
                                <i class="nav-icon fas fa-business-time"></i>
                                <p>
                                    Experience
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Education/Index/" class="nav-link">
                                <i class="nav-icon fas fa-university"></i>
                                <p>
                                    Education
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Skills/Index/" class="nav-link">
                                <i class="nav-icon fas fa-code"></i>
                                <p>
                                    Skills
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Interest/Index/" class="nav-link">
                                <i class="nav-icon fas fa-chess"></i>
                                <p>
                                    Interetst
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Certificate/Index/" class="nav-link">
                                <i class="nav-icon fas fa-award"></i>
                                <p>
                                    Certificate
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Contact/Index/" class="nav-link">
                                <i class="nav-icon fas fa-mail-bulk"></i>
                                <p>
                                    Contact
                                </p>
                            </a>
                        </li>
                          <li class="nav-item has-treeview">
                          <a href="/SocialMedia/Index/" class="nav-link">
                                <i class="nav-icon fas fa-photo-video"></i>
                                <p>
                                    Social Media
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Admin/Index/" class="nav-link">
                                <i class="nav-icon fas fa-wrench"></i>
                                <p>
                                    Settings
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Login/LoginOut/" class="nav-link">
                                <i class="nav-icon fas fa-sign-out-alt"></i>
                                <p>
                                    Login Out
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <div class="content-wrapper">
            <section class="content">
                <div>

                    @RenderBody()

                </div>
            </section>

        </div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/AdminLTE-3.0.4/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/AdminLTE-3.0.4/dist/js/demo.js"></script>
</body>
</html>