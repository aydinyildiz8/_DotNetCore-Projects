@model MyCvProject.Models.Entities.Skill

@{
    ViewData["Title"] = "UpdateSkills";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Yetenek Düzenleme Sayfası</h4>
<form method="post">
    @Html.Label("ID")
    @Html.TextBoxFor(x => x.Id, new {@class = "form-control"})
    <br />

    @Html.Label("Yetenek")
    @Html.TextBoxFor(x => x.Yetenek, new {@class = "form-control"})
    <br />

    @Html.Label("Alan")
    @Html.TextBoxFor(x => x.Oran, new {@class = "form-control"})
    <br />

    <button type="submit" class="btn btn-info">GÜNCELLE</button>
</form>
